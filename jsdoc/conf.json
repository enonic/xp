{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "source": {
    "include": [
      "jsdoc/global.js",
      "modules/lib"
    ]
  },
  "plugins": [
    "plugins/example",
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false,
      "outputSourcePath": false
    }
  },
  "opts": {
    "recurse": true,
    "destination": "build/docs/jsdoc",
    "readme": "jsdoc/index.md"
  }
}
