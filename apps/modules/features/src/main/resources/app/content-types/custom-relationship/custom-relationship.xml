<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<content-type>
  <!-- This content type references custom relationship-types. -->
  <display-name>Custom Relationship</display-name>
  <super-type>base:structured</super-type>
  <form>
    <input name="cited-in" type="ContentSelector">
      <label>Cited In</label>
      <custom-text/>
      <help-text/>
      <occurrences minimum="0" maximum="0"/>
      <config>
        <relationship-type>com.enonic.xp.modules.features:citation</relationship-type>
      </config>
    </input>
    <input name="media-in-use" type="ContentSelector">
      <label>Media in use</label>
      <occurrences minimum="1" maximum="1"/>
      <config>
        <relationship-type>com.enonic.xp.modules.features:media-content</relationship-type>
      </config>
    </input>
  </form>
</content-type>
