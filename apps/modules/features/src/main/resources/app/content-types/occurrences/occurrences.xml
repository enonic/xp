<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<content-type>
  <display-name>Occurrences</display-name>
  <super-type>base:structured</super-type>
  <form>
    <input name="textLineUnlimitedOccurrences" type="TextLine">
      <label>TextLine with min 0 and unlimited max</label>
      <occurrences minimum="0" maximum="0"/>
    </input>
    <input name="textLineUnrequiredMaxOne" type="TextLine">
      <label>TextLine with min 0 and max 1</label>
      <occurrences minimum="0" maximum="1"/>
    </input>
    <input name="textLineRequiredAndUnlimitedOccurrences" type="TextLine">
      <label>TextLine with min 1 and unlimited max</label>
      <occurrences minimum="1" maximum="0"/>
    </input>
    <input name="dateRequired" type="Date">
      <label>Date required</label>
      <occurrences minimum="1" maximum="0"/>
    </input>
    <input name="dateTimeRequired" type="DateTime">
      <label>DateTime required</label>
      <occurrences minimum="1" maximum="0"/>
    </input>
    <input name="timeRequired" type="Time">
      <label>Time required</label>
      <occurrences minimum="1" maximum="0"/>
    </input>
    <input name="longRequired" type="Long">
      <label>Long required</label>
      <occurrences minimum="1" maximum="0"/>
    </input>
    <input name="textLineMin2Max4" type="TextLine">
      <label>TextLine with min 2 and max 4</label>
      <occurrences minimum="2" maximum="4"/>
    </input>
    <item-set name="formItemSetUnrequiredAndMaxOneOccurrence">
      <label>Set with min 0 and max 1</label>
      <items>
        <input name="myText" type="TextLine">
          <label>My Text</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
        <input name="myOtherText" type="TextLine">
          <label>My other Text</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
      </items>
      <occurrences minimum="0" maximum="1"/>
    </item-set>
    <item-set name="formItemSetRequiredAndMaxOneOccurrence">
      <label>Set with min 1 and max 1</label>
      <items>
        <input name="myRequiredText" type="TextLine">
          <label>My required Text</label>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input name="myOtherText" type="TextLine">
          <label>My other Text</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
      </items>
      <occurrences minimum="1" maximum="1"/>
    </item-set>
    <item-set name="formItemSetUnrequiredAndUnlimitedOccurrences">
      <label>Set with min 0 and unlimited max</label>
      <items>
        <input name="myText" type="TextLine">
          <label>My Text</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
        <input name="myOtherText" type="TextLine">
          <label>My other Text</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
      </items>
      <occurrences minimum="0" maximum="0"/>
    </item-set>
    <item-set name="formItemSetRequiredAndUnlimitedOccurrences">
      <label>Set with min 1 and unlimited max</label>
      <items>
        <input name="myText" type="TextLine">
          <label>My Text</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
        <input name="myOtherText" type="TextLine">
          <label>My other Text</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
      </items>
      <occurrences minimum="1" maximum="0"/>
    </item-set>
    <item-set name="formItemSetRequired2AndMax4Occurrences">
      <label>Set with min 2 and max 4</label>
      <items>
        <input name="myText" type="TextLine">
          <label>My Text</label>
          <occurrences minimum="0" maximum="1"/>
        </input>
        <input name="myOtherText" type="TextLine">
          <label>My other Text</label>
          <occurrences minimum="1" maximum="1"/>
        </input>
      </items>
      <occurrences minimum="2" maximum="4"/>
    </item-set>
  </form>
</content-type>
