<?xml version="1.0" encoding="UTF-8"?>
<site-template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../schema/model.xsd">
  <display-name>Enonic Intranet</display-name>
  <description>A social intranet for the Enterprise</description>
  <url>http://www.enonic.com/intranet</url>
  <vendor>
    <name>Enonic</name>
    <url>https://www.enonic.com</url>
  </vendor>
  <modules>
    <module>com.enonic.intranet-1.0.0</module>
    <module>com.company.sampleModule-1.1.0</module>
    <module>com.company.theme.someTheme-1.4.1</module>
    <module>com.enonic.resolvers-1.0.0</module>
  </modules>
  <content-filter>
    <deny>*</deny>
    <allow>mymodule-1.0.0:com.enonic.tweet</allow>
    <allow>mymodule-1.0.0:system.folder</allow>
    <deny>mymodule-1.0.0:system.folder.example</deny>
  </content-filter>
</site-template>