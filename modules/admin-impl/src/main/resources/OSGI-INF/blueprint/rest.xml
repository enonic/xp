<?xml version="1.0" encoding="UTF-8"?>
<blueprint xmlns="http://www.osgi.org/xmlns/blueprint/v1.0.0">

  <reference id="startupInitializer" interface="com.enonic.wem.core.initializer.StartupInitializer"/>

  <reference id="securityService" interface="com.enonic.wem.api.security.SecurityService"/>

  <reference id="contentService" interface="com.enonic.wem.api.content.ContentService"/>

  <reference id="contentTypeService" interface="com.enonic.wem.api.schema.content.ContentTypeService"/>

  <reference id="mixinService" interface="com.enonic.wem.api.schema.mixin.MixinService"/>

  <reference id="moduleService" interface="com.enonic.wem.api.module.ModuleService"/>

  <reference id="relationshipService" interface="com.enonic.wem.api.relationship.RelationshipService"/>

  <reference id="relationshipTypeService" interface="com.enonic.wem.api.schema.relationship.RelationshipTypeService"/>

  <reference id="pageService" interface="com.enonic.wem.api.content.page.PageService"/>

  <reference id="pageTemplateService" interface="com.enonic.wem.api.content.page.PageTemplateService"/>

  <reference id="pageDescriptorService" interface="com.enonic.wem.api.content.page.PageDescriptorService"/>

  <reference id="partDescriptorService" interface="com.enonic.wem.api.content.page.region.PartDescriptorService"/>

  <reference id="layoutDescriptorService" interface="com.enonic.wem.api.content.page.region.LayoutDescriptorService"/>

  <reference id="nodeService" interface="com.enonic.wem.api.node.NodeService"/>

  <reference id="exportService" interface="com.enonic.wem.api.export.ExportService"/>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.tools.ToolsResource">
      <property name="startupInitializer" ref="startupInitializer"/>
      <property name="nodeService" ref="nodeService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.export.ExportResource">
      <property name="exportService" ref="exportService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.content.ContentMediaResource">
      <property name="contentService" ref="contentService"/>
      <property name="contentTypeService" ref="contentTypeService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.content.ContentImageResource">
      <property name="contentService" ref="contentService"/>
      <property name="contentTypeService" ref="contentTypeService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.content.ContentIconResource">
      <property name="contentService" ref="contentService"/>
    </bean>
  </service>


  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.module.ModuleResource">
      <property name="bundleContext" ref="blueprintBundleContext"/>
      <property name="moduleService" ref="moduleService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.relationship.RelationshipResource">
      <property name="relationshipService" ref="relationshipService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.schema.relationship.RelationshipTypeResource">
      <property name="relationshipTypeService" ref="relationshipTypeService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.content.ContentResource">
      <property name="contentService" ref="contentService"/>
      <property name="contentTypeService" ref="contentTypeService"/>
      <property name="mixinService" ref="mixinService"/>
      <property name="securityService" ref="securityService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.security.SecurityResource">
      <property name="securityService" ref="securityService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.content.page.PageResource">
      <property name="contentTypeService" ref="contentTypeService"/>
      <property name="mixinService" ref="mixinService"/>
      <property name="pageService" ref="pageService"/>
      <property name="securityService" ref="securityService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.schema.mixin.MixinResource">
      <property name="mixinService" ref="mixinService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.schema.content.ContentTypeResource">
      <property name="contentTypeService" ref="contentTypeService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.content.page.PageTemplateResource">
      <property name="pageTemplateService" ref="pageTemplateService"/>
      <property name="contentService" ref="contentService"/>
      <property name="contentTypeService" ref="contentTypeService"/>
      <property name="mixinService" ref="mixinService"/>
      <property name="securityService" ref="securityService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.content.page.PageDescriptorResource">
      <property name="pageDescriptorService" ref="pageDescriptorService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.content.page.part.PartDescriptorResource">
      <property name="partDescriptorService" ref="partDescriptorService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.content.page.layout.LayoutDescriptorResource">
      <property name="layoutDescriptorService" ref="layoutDescriptorService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.auth.AuthResource">
      <property name="securityService" ref="securityService"/>
    </bean>
  </service>

  <service auto-export="interfaces">
    <bean class="com.enonic.wem.admin.rest.resource.status.InfoResource"/>
  </service>

</blueprint>
