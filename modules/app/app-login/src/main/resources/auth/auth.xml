<?xml version="1.0" encoding="UTF-8"?>
<auth>
  <config>
    <input name="userstore" type="UserStoreSelector">
      <label>Userstore</label>
      <occurrences minimum="0" maximum="1"/>
      <config>
        <relationship-type>system:reference</relationship-type>
      </config>
    </input>
    <item-set name="background">
      <label>Background</label>
      <occurrences minimum="1" maximum="1"/>
      <items>
        <input name="application" type="TextLine">
          <label>Application</label>
          <occurrences minimum="1" maximum="1"/>
        </input>
        <input name="path" type="TextLine">
          <label>Asset path</label>
          <occurrences minimum="1" maximum="1"/>
        </input>
      </items>
    </item-set>
  </config>
</auth>
