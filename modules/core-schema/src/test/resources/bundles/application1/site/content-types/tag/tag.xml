<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<content-type>
  <display-name>Tag</display-name>
  <super-type>base:structured</super-type>
  <is-abstract>false</is-abstract>
  <is-final>true</is-final>
  <allow-child-content>true</allow-child-content>
  <form>
    <input name="tag_unlimited" type="Tag">
      <label>Tag, unlimited occurrences</label>
      <immutable>false</immutable>
      <indexed>true</indexed>
      <custom-text>Custom text</custom-text>
      <occurrences minimum="0" maximum="0"/>
      <help-text>Some help text</help-text>
    </input>
    <input name="tag_max5" type="Tag">
      <label>Tag, max 5 occurrences</label>
      <immutable>false</immutable>
      <indexed>true</indexed>
      <custom-text>Custom text</custom-text>
      <occurrences minimum="0" maximum="5"/>
      <help-text>Some help text</help-text>
    </input>
    <input name="tag_min_2_max5" type="Tag">
      <label>Tag, min 2 max 5 occurrences</label>
      <immutable>false</immutable>
      <indexed>true</indexed>
      <custom-text>Custom text</custom-text>
      <occurrences minimum="2" maximum="5"/>
      <help-text>Some help text</help-text>
    </input>
  </form>
</content-type>
