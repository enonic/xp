<?xml version="1.0" encoding="UTF-8"?>
<mixin>
  <name>my_set</name>
  <module>mymodule</module>
  <display-name>My Mixin</display-name>
  <items>
    <form-item-set name="my_set">
      <label>My set</label>
      <immutable>false</immutable>
      <custom-text>Custom text</custom-text>
      <help-text>Custom text</help-text>
      <occurrences minimum="1" maximum="3" />
      <items>
        <input type="TextLine" name="my_input">
          <label>My input</label>
          <immutable>false</immutable>
          <indexed>false</indexed>
          <custom-text />
          <help-text />
          <occurrences minimum="0" maximum="1" />
        </input>
      </items>
    </form-item-set>
  </items>
</mixin>