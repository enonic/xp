{
  "compilerOptions": {
    "lib": ["es6"],
    "removeComments": false,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "types": [
      "base"
    ],
    "sourceMap": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": [
    "./*.d.ts"
  ]
}
