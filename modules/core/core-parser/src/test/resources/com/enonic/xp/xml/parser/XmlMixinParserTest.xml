<?xml version="1.0"?>
<mixin xmlns="urn:enonic:xp:model:1.0">
  <display-name i18n="key.display-name">display name</display-name>
  <description i18n="key.description">description</description>
  <form>
    <item-set name="mySet">
      <immutable>false</immutable>
      <occurrences minimum="0" maximum="1"/>
      <items>
        <field-set>
          <label>My field set</label>
          <items>
            <input type="TextLine" name="myTextLine">
              <label>label</label>
              <immutable>false</immutable>
              <indexed>false</indexed>
              <occurrences minimum="0" maximum="1"/>
            </input>
          </items>
        </field-set>
      </items>
    </item-set>
  </form>
</mixin>
