{
  "index": {
    "number_of_shards": 5,
    "number_of_replicas": 0
  },
  "analysis": {
    "filter": {
      "front_ngram": {
        "side": "front",
        "max_gram": 3,
        "min_gram": 2,
        "type": "edgeNGram"
      },
      "ngram": {
        "type": "nGram",
        "min_gram": "2",
        "max_gram": "12",
        "token_chars": [
          "letter",
          "digit"
        ]
      }
    },
    "analyzer": {
      "default_search": {
        "tokenizer": "keyword",
        "filter": [
          "lowercase",
          "asciifolding"
        ]
      },
      "keywordlowercase": {
        "type": "custom",
        "tokenizer": "keyword",
        "filter": ["lowercase"]
      },
      "keywordasciilowercase": {
        "type": "custom",
        "tokenizer": "keyword",
        "filter": [
          "lowercase",
          "asciifolding"
        ]
      },
      "ngram_index_front": {
        "type": "custom",
        "filter": [
          "lowercase",
          "asciifolding",
          "word_delimiter",
          "front_ngram"
        ],
        "tokenizer": "standard"
      },
      "ngram_index": {
        "type": "custom",
        "filter": [
          "lowercase",
          "asciifolding",
          "word_delimiter",
          "ngram"
        ],
        "tokenizer": "standard"
      },
      "ngram_search_default": {
        "type": "custom",
        "filter": [
          "lowercase",
          "asciifolding",
          "word_delimiter"
        ],
        "tokenizer": "standard"
      },
      "fulltext_search_default": {
        "type": "custom",
        "filter": [
          "lowercase",
          "asciifolding",
          "word_delimiter"
        ],
        "tokenizer": "standard"
      },
      "document_index_default": {
        "type": "custom",
        "filter": [
          "lowercase",
          "asciifolding",
          "word_delimiter"
        ],
        "tokenizer": "standard"
      },
      "all_field_analyzer": {
        "type": "custom",
        "filter": [
          "lowercase",
          "asciifolding",
          "front_ngram"
        ],
        "tokenizer": "standard"
      }
    }
  },
  "analyzer": {
    "default_search": {
      "tokenizer": "fisk",
      "filter": [
        "lowercase",
        "ost"
      ]
    }
  }
}
