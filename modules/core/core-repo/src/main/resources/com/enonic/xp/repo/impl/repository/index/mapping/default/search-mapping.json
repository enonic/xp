{
  "date_detection": false,
  "numeric_detection": false,
  "dynamic_templates": [
    {
      "template_fulltext": {
        "match": "*@_analyzed",
        "mapping": {
          "type": "text",
          "analyzer": "document_index_default"
        }
      }
    },
    {
      "template_path": {
        "match": "*@_path",
        "mapping": {
          "type": "text",
          "analyzer": "path_analyzer"
        }
      }
    },
    {
      "template_ngram": {
        "match": "*@_ngram",
        "mapping": {
          "type": "text",
          "analyzer": "ngram_index_front",
          "search_analyzer": "document_index_default"
        }
      }
    },
    {
      "template_userdata_date": {
        "match": "*@_datetime",
        "mapping": {
          "type": "date"
        }
      }
    },
    {
      "template_userdata_number": {
        "match": "*@_number",
        "mapping": {
          "type": "double"
        }
      }
    },
    {
      "template_userdata_orderby": {
        "match": "*@_orderby",
        "mapping": {
          "type": "keyword",
          "normalizer": "lowercase",
          "ignore_above": 3072
        }
      }
    },
    {
