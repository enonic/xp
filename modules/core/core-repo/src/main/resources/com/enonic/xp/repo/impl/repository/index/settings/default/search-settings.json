        ]
      },
      "language_analyzer_es": {
        "tokenizer": "standard",
        "filter": [
          "lowercase",
          "spanish_stemmer"
        ]
      },
      "language_analyzer_sv": {
        "tokenizer": "standard",
        "filter": [
          "lowercase",
